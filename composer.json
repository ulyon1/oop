{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "beberlei/assert": "^2.8",
        "doctrine/dbal": "^2.6",
        "mathiasverraes/classfunctions": "^1.1"
    },
    "require-dev": {
        "ramsey/uuid-console": "^1.1",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [],
    "scripts": {},
    "extra": {}
}
